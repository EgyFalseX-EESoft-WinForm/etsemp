                         TBLGehawork ON CDMarahel.marhala_code = TBLGehawork.marhalaid RIGHT OUTER JOIN
                         TBL_Emp ON TBLGehawork.GehaworkId = TBL_Emp.marhala_code LEFT OUTER JOIN
